{
  "name": "mservice-calendar",
  "description": "Calendar microservice",
  "main": "./lib/index.js",
  "scripts": {
    "doc": "cd schemas && apidoc -i ./../src/actions -v -o ./../docs",
    "compile": "babel -d ./lib ./src",
    "docker-release": "./docker-release.sh",
    "lint": "eslint ./src",
    "prepublish": "yarn compile",
    "semantic-release": "semantic-release",
    "start": "mfleet",
    "pretest": "yarn compile",
    "test": "rimraf ./coverage && yarn lint && ./test/docker.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/mservice-calendar.git"
  },
  "keywords": [
    "calendar",
    "microservice"
  ],
  "author": "Dmitry Gorbunov <atskiisotona@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/makeomatic/mservice-calendar/issues"
  },
  "homepage": "https://github.com/makeomatic/mservice-calendar#readme",
  "dependencies": {
    "@microfleet/core": "^13.3.2",
    "@microfleet/transport-amqp": "^13.1.2",
    "@microfleet/validation": "^8.1.0",
    "bluebird": "^3.5.0",
    "bunyan": "^1.8.9",
    "common-errors": "^1.0.0",
    "firebase-admin": "^5.12.0",
    "hapi": "^17.6.2",
    "is": "^3.2.1",
    "knex": "^0.15.2",
    "lodash": "^4.17.2",
    "luxon": "^1.4.5",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.10",
    "ms-conf": "^3.1.2",
    "pg": "^7.6.0",
    "rrule": "^2.2.0",
    "stdout-stream": "^1.4.0",
    "uid2": "0.0.3",
    "uuid": "^3.0.1",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@google-cloud/storage": "^0.8.0",
    "@makeomatic/deploy": "^8.2.3",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/exec": "^3.3.0",
    "@semantic-release/git": "^7.0.5",
    "apidoc": "^0.17.5",
    "apidoc-plugin-schema": "^0.1.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "eslint": "^5.8.0",
    "eslint-config-makeomatic": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^4.0.1",
    "hash-files": "^1.1.1",
    "json": "^9.0.6",
    "mime-types": "^2.1.15",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^7.1.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 4.0.0"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $HUSKY_GIT_PARAMS"
    }
  }
}
