{
  "name": "mservice-calendar",
  "description": "Calendar microservice",
  "main": "./lib/index.js",
  "scripts": {
    "doc": "cd schemas && apidoc -i ./../src/actions -v -o ./../docs",
    "flow": "flow check; test $? -eq 0 -o $? -eq 2",
    "compile": "flow-remove-types src/ --out-dir lib/",
    "docker-release": "./docker-release.sh",
    "lint": "eslint ./src",
    "prepublish": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "node ./bin/mservice.js | bunyan -o short",
    "pretest": "npm run compile",
    "test": "npm run lint && npm run flow && ./test/docker.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeomatic/mservice-calendar.git"
  },
  "keywords": [
    "calendar",
    "microservice"
  ],
  "author": "Dmitry Gorbunov <atskiisotona@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/makeomatic/mservice-calendar/issues"
  },
  "homepage": "https://github.com/makeomatic/mservice-calendar#readme",
  "dependencies": {
    "bluebird": "^3.4.6",
    "bunyan": "^1.8.5",
    "common-errors": "^1.0.0",
    "hapi": "^16.0.1",
    "is": "^3.2.0",
    "knex": "^0.12.6",
    "lodash": "^4.17.2",
    "moment": "^2.17.0",
    "moment-timezone": "^0.5.10",
    "ms-amqp-transport": "^7.0.1",
    "ms-conf": "^2.3.0",
    "ms-validation": "^3.0.0",
    "mservice": "^4.7.2",
    "node-uuid": "^1.4.7",
    "pg": "^6.1.0",
    "pg-native": "^1.10.0",
    "rrule": "^2.1.0",
    "uid2": "0.0.3"
  },
  "devDependencies": {
    "apidoc": "^0.16.1",
    "apidoc-plugin-schema": "^0.1.0",
    "babel-eslint": "^7.1.1",
    "codecov": "^1.0.1",
    "dotenv": "^2.0.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "flow-bin": "^0.36.0",
    "flow-remove-types": "^1.0.5",
    "istanbul": "=0.4.4",
    "json": "^9.0.4",
    "latest-version-cli": "^1.0.0",
    "mocha": "^3.2.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "semantic-release": "^6.3.2"
  },
  "release": {
    "verifyConditions": []
  }
}
