{
  "extends": "airbnb-base",
  "plugins": [
      "promise"
  ],
  "rules": {
    "promise/param-names": 2,
    "promise/always-return": 2,
    "promise/catch-or-return": 2,
    "promise/no-native": 2,
    "global-require": 0,
    "no-underscore-dangle": 0,
    "prefer-arrow-callback": 0,
    "no-param-reassign": [2,{"props": false}],
    "max-len": [2, 150]
  }
}
